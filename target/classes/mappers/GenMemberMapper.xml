<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.care.root.mybatis.GenMemberMapper">
	<resultMap type="com.care.root.dto.GenMemberDTO" id="genmember">
		<id property="id" column="n_id" />
		<result property="pwd" column="n_pwd"/>
		<result property="name" column="n_name"/>
		<result property="birth" column="birth"/>
		<result property="email" column="n_email"/>
		<result property="addr" column="n_addr"/>
		<result property="tel" column="n_tel"/>
	</resultMap>
	<select id="getMember" resultMap="genmember">
		select * from genmember where n_id=#{id}
	</select>
	<select id="getList" resultMap="genmember">
		select * from genmember
	</select>
	<insert id="genregister">
		insert into genmember(n_id, n_pwd, n_name,birth, n_email, n_addr, n_tel) values(#{id},#{pwd},#{name},#{birth},#{email},#{addr},#{tel})
	</insert>
	<update id="genmodify">
		update genmember set n_pwd=#{pwd}, n_name=#{name},birth=#{birth}, n_email=#{email}, n_addr=#{addr}, n_tel=#{tel} where n_id=#{id}
	</update>
</mapper>